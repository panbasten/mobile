	<Control Class="UFGauge">
		<Property Name="Value" Type="Double" />
		<Property Name="NumberFormat" Type="String" />
		<Property Name="BindEnvVar" Type="String" />
		<Property Name="LinkTo" Type="String" />
		<Property Name="ScaleMin" Type="Double" />
		<Property Name="ScaleMax" Type="Double" />
		<Property Name="ScaleMajorNumber" Type="Integer" />
		<Property Name="ScaleMinorNumber" Type="Integer" />
		<Property Name="Sections" Type="Array:Object" Editor="PropertyPage">
			<Format Color="#FF0000" Condition="[Value] &lt; 60" />
			<Format Color="#00FF00" Condition="[Value] &gt;= 60 and [Value] &lt; 80" />
			<Format Color="#0000FF" Condition="[Value] &gt; 80" />
		</Property>
	</Control>
	
